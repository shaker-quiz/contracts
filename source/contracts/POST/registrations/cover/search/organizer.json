{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "game": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "registration": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "registration/status": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Created",
          "Confirmed",
          "Cancelled"
        ]
      },
      "uniqueItems": true
    },
    "registration/lineup": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Main",
          "Reserve"
        ]
      },
      "uniqueItems": true
    },
    "registration/interval": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "1 day",
          "3 days",
          "1 week",
          "1 month",
          "3 months",
          "6 months",
          "1 year",
          "all time"
        ]
      },
      "uniqueItems": true
    }
  },
  "required": [
    "registration",
    "registration/status",
    "registration/lineup",
    "registration/interval"
  ],
  "additionalProperties": false
}
