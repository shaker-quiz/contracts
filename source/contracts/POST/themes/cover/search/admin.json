{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "theme": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "theme/status": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Active",
          "Archive"
        ]
      },
      "uniqueItems": true
    },
    "cover": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    }
  },
  "required": [
    "theme",
    "theme/status",
    "cover"
  ],
  "additionalProperties": false
}
