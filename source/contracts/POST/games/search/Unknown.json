{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "city": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "venue": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "game": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "game/status": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "APPROVED",
          "ARCHIVE",
          "CLOSED",
          "FINISHED",
          "FORINVITES",
          "IS_RESERVE",
          "MODERATION",
          "PUBLISHED",
          "REJECTED"
        ]
      },
      "uniqueItems": true
    },
    "registration": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    }
  },
  "required": [
    "city",
    "venue",
    "game",
    "game/status",
    "registration"
  ],
  "additionalProperties": false
}
