{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "cities": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "venues": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "games": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "ids": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "statuses": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Created",
          "Confirmed",
          "Cancelled"
        ]
      },
      "uniqueItems": true
    },
    "lineups": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Main",
          "Reserve"
        ]
      },
      "uniqueItems": true
    }
  },
  "required": [
    "cities",
    "venues",
    "games",
    "ids",
    "statuses",
    "lineups"
  ],
  "additionalProperties": false
}
