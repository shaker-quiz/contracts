{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid"
      },
      "status": {
        "type": "string",
        "enum": [
          "Active",
          "Archive"
        ]
      },
      "audience": {
        "type": "string",
        "enum": [
          "Open",
          "Adults"
        ]
      }
    },
    "required": [
      "id"
    ],
    "additionalProperties": false
  },
  "uniqueItems": true
}
