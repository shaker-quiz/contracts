{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "ids": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "statuses": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Active",
          "Archive"
        ]
      },
      "uniqueItems": true
    }
  },
  "required": [
    "ids",
    "statuses"
  ],
  "additionalProperties": false
}
