{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid"
      },
      "status": {
        "type": "string",
        "enum": [
          "APPROVED",
          "ARCHIVE",
          "CLOSED",
          "FINISHED",
          "FORINVITES",
          "IS_RESERVE",
          "MODERATION",
          "PUBLISHED",
          "REJECTED"
        ]
      }
    },
    "required": [
      "id"
    ],
    "additionalProperties": false
  },
  "uniqueItems": true
}
