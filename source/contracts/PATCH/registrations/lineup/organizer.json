{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid"
      },
      "lineup": {
        "lineup": {
          "type": "string",
          "enum": [
            "Main",
            "Reserve"
          ]
        }
      }
    },
    "required": [
      "id",
      "lineup"
    ]
  },
  "uniqueItems": true
}
