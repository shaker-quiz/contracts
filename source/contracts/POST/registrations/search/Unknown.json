{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "city": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "venue": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "game": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "registration": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "registration/status": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Created",
          "Confirmed",
          "Cancelled"
        ]
      },
      "uniqueItems": true
    },
    "registration/lineup": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Main",
          "Reserve"
        ]
      },
      "uniqueItems": true
    }
  },
  "required": [
    "city",
    "venue",
    "game",
    "registration",
    "registration/status",
    "registration/lineup"
  ],
  "additionalProperties": false
}
