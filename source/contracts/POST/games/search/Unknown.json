{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "cities": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "venues": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "ids": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "uniqueItems": true
    },
    "statuses": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "APPROVED",
          "ARCHIVE",
          "CLOSED",
          "FINISHED",
          "FORINVITES",
          "IS_RESERVE",
          "MODERATION",
          "PUBLISHED",
          "REJECTED"
        ]
      },
      "uniqueItems": true
    }
  },
  "required": [
    "cities",
    "venues",
    "ids",
    "statuses"
  ],
  "additionalProperties": false
}
