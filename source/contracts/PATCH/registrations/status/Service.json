{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid"
      },
      "status": {
        "type": "string",
        "enum": [
          "Created",
          "Confirmed",
          "Cancelled"
        ]
      }
    },
    "required": [
      "id",
      "status"
    ]
  },
  "uniqueItems": true
}
